{
    "name"       : "narrowspark/arr",
    "type"       : "library",
    "description": "A collection of utilities to manipulate arrays.",
    "keywords"   : [
        "array", "manipulate"
    ],
    "license"    : "MIT",
    "homepage"   : "https://github.com/narrowspark/arr",
    "support"    : {
        "issues"      : "https://github.com/narrowspark/arr/issues",
        "source"      : "https://github.com/narrowspark/arr"
    },
    "authors"    : [
        {
            "name"    : "Daniel Bannert",
            "email"   : "d.bannert@anolilab.de",
            "homepage": "http://anolilab.de",
            "role"    : "Developer"
        }
    ],
    "require": {
        "php"                             : "^7.0"
    },
    "require-dev": {
        "narrowspark/php-cs-fixer-config" : "^2.0",
        "phpbench/phpbench"               : "^0.11",
        "phpunit/phpunit"                 : "^5.1"
    },
    "autoload": {
        "psr-4": {
            "Narrowspark\\Arr\\"          : "src/Arr/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Narrowspark\\Arr\\Tests\\"   : "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master"                  : "1.0-dev"
        }
    },
    "suggest": {
    },
    "minimum-stability"                   : "dev",
    "prefer-stable"                       : true
}
